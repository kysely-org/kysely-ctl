{
	"name": "kysely-ctl",
	"version": "0.12.2",
	"type": "module",
	"bin": {
		"kysely": "./dist/bin.js"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist"
	],
	"description": "Command-line tool for Kysely",
	"author": "Igal Klebanov <igalklebanov@gmail.com> (https://github.com/igalklebanov)",
	"license": "MIT",
	"repository": "git://github.com/kysely-org/kysely-ctl.git",
	"scripts": {
		"biome": "biome",
		"build": "tsup",
		"check:dts": "attw --pack .",
		"prepublishOnly": "biome check && pnpm build && pnpm check:dts",
		"release:preview": "pkg-pr-new publish --template './examples/node-esm'",
		"start": "pnpm build --watch",
		"test": "vitest"
	},
	"dependencies": {
		"c12": "^2.0.1",
		"citty": "^0.1.6",
		"consola": "^3.4.0",
		"jiti": "^2.4.2",
		"nypm": "^0.5.2",
		"ofetch": "^1.4.1",
		"pathe": "^2.0.2",
		"pkg-types": "^1.3.1",
		"std-env": "^3.8.0"
	},
	"peerDependencies": {
		"kysely": ">=0.18.1 <0.29.0",
		"kysely-postgres-js": "^2"
	},
	"peerDependenciesMeta": {
		"kysely-postgres-js": {
			"optional": true
		}
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.18.1",
		"@arktype/attest": "^0.46.0",
		"@biomejs/biome": "1.9.4",
		"@tsconfig/node24": "^24.0.0",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^22.15.18",
		"better-sqlite3": "^11.10.0",
		"kysely": "^0.28.2",
		"kysely-postgres-js": "^2.0.0",
		"pkg-pr-new": "^0.0.50",
		"postgres": "^3.4.5",
		"tsup": "^8.5.0",
		"type-fest": "^4.41.0",
		"typescript": "^5.8.3",
		"vitest": "^3.1.3"
	},
	"engines": {
		"node": ">=18"
	},
	"keywords": [
		"cli",
		"ctl",
		"kysely",
		"migration",
		"seeding",
		"codegen"
	],
	"packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"better-sqlite3",
			"esbuild"
		]
	}
}
