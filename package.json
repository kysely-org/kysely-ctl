{
	"name": "kysely-ctl",
	"version": "0.14.0",
	"type": "module",
	"bin": {
		"kysely": "./dist/bin.js"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist",
		"assets"
	],
	"description": "Command-line tool for Kysely",
	"author": "Igal Klebanov <igalklebanov@gmail.com> (https://github.com/igalklebanov)",
	"license": "MIT",
	"repository": "git://github.com/kysely-org/kysely-ctl.git",
	"scripts": {
		"biome": "biome",
		"build": "tsup",
		"check:exports": "attw --pack .",
		"prepublishOnly": "biome check && pnpm build && pnpm check:exports",
		"start": "pnpm build --watch",
		"test": "vitest"
	},
	"dependencies": {
		"c12": "^3.2.0",
		"citty": "^0.1.6",
		"consola": "^3.4.2",
		"jiti": "^2.5.1",
		"nypm": "^0.6.1",
		"ofetch": "^1.4.1",
		"pathe": "^2.0.3",
		"pkg-types": "^2.2.0",
		"std-env": "^3.9.0",
		"tsconfck": "^3.1.6"
	},
	"peerDependencies": {
		"kysely": ">=0.18.1 <0.29.0",
		"kysely-postgres-js": "^2"
	},
	"peerDependenciesMeta": {
		"kysely-postgres-js": {
			"optional": true
		}
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.18.2",
		"@arktype/attest": "^0.46.0",
		"@biomejs/biome": "2.1.3",
		"@tsconfig/node24": "^24.0.1",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^24.2.0",
		"better-sqlite3": "^12.2.0",
		"kysely": "^0.28.4",
		"kysely-postgres-js": "^2.0.0",
		"postgres": "^3.4.7",
		"tsup": "^8.5.0",
		"type-fest": "^4.41.0",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	},
	"engines": {
		"node": ">=20"
	},
	"keywords": [
		"cli",
		"ctl",
		"kysely",
		"migration",
		"seeding",
		"codegen"
	],
	"packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
