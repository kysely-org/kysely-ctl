{
  "name": "kysely-ctl",
  "version": "0.3.0",
  "type": "module",
  "bin": {
    "kysely": "./dist/bin.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "description": "Command-line tool for Kysely",
  "author": "Igal Klebanov <igalklebanov@gmail.com> (https://github.com/igalklebanov)",
  "license": "MIT",
  "repository": "git://github.com/kysely-org/kysely-ctl.git",
  "scripts": {
    "build": "tsup",
    "check:dts": "attw --pack .",
    "prepublishOnly": "pnpm build && pnpm check:dts",
    "start": "pnpm build --watch"
  },
  "peerDependencies": {
    "c12": "^1.10",
    "citty": "^0.1",
    "consola": "^3.2",
    "kysely": "^0.27",
    "kysely-postgres-js": "^2",
    "nypm": "^0.3",
    "pathe": "^1.1",
    "pkg-types": "^1.1",
    "std-env": "^3.7",
    "tsx": "^4.9"
  },
  "peerDependenciesMeta": {
    "kysely-postgres-js": {
      "optional": true
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/better-sqlite3": "^7.6.10",
    "@types/node": "^20.12.12",
    "better-sqlite3": "^9.6.0",
    "c12": "^1.10.0",
    "citty": "^0.1.6",
    "consola": "^3.2.3",
    "kysely": "^0.27.3",
    "kysely-postgres-js": "^2.0.0",
    "mysql2": "^3.9.7",
    "nypm": "^0.3.8",
    "pathe": "^1.1.2",
    "pg": "^8.11.5",
    "pkg-types": "^1.1.1",
    "postgres": "^3.4.4",
    "std-env": "^3.7.0",
    "tarn": "^3.0.2",
    "tedious": "^18.2.0",
    "tsup": "^8.0.2",
    "tsx": "^4.9.5",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "cli",
    "ctl",
    "kysely",
    "migration",
    "seeding",
    "codegen"
  ],
  "packageManager": "pnpm@9.0.6+sha1.648f6014eb363abb36618f2ba59282a9eeb3e879"
}
